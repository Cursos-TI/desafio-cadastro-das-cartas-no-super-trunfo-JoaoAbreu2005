#include <stdio.h>
int main(){

//Pais Argentina,
char NomeDoPaís[10]= "Argentina";
printf("País: %s \n",NomeDoPaís);

//Estado
char EstadoDoPais[15] = "Bueno Aires" ;
printf("Estado: %s \n", EstadoDoPais);

//Codigo da carta
char codigodacarta[4] = "A01";
printf("Código da carta: %s \n", codigodacarta);

//Nome da cidade
char Cidade[10]="La Plata";
printf("Cidade: %s \n",Cidade);

//População
float NumeroDeAbitantes=923.715;
printf("População :%.3f \n",NumeroDeAbitantes);

//Área
int Area = 	940;
printf("Área em Km²: %d \n", Area);

//Pib
//o valor da econimia esta em pesos Argentina se converter em dolár e 640,59 bilhões USD
float econimia=678.000000000000;
printf("Pib do País: %.12f \n",econimia);

//Turisticos
int Pontos= 50;
printf("Pontos Turisticos: %d \n",Pontos);

//Densidade
float Densidade;
Densidade = NumeroDeAbitantes / Area;
printf("Densidade:%f\n", Densidade);

//PIB Capital
float PIBCapital;
PIBCapital = econimia / NumeroDeAbitantes;
printf("PIB capital: %f \n", PIBCapital);






}